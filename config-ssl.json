{
  "interactiveLogin": true,
  "httpServer": {
    "type": "NettyWrapper"
  },
  "tokenCallbacks": [
    {
      "issuerId": "default",
      "requestMappings": [
        {
          "requestParam": "grant_type",
          "match": "client_credentials",
          "claims": {
            "sub": "demo-sub",
            "aud": ["demo-aud"]
          }
        }
      ]
    }
  ]
}
